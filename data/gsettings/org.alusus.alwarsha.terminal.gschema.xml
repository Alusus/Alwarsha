<schemalist>
  <schema id="org.alusus.alwarsha.terminal" path="/org/alusus/alwarsha/terminal/" gettext-domain="alwarsha">
    <key name="allow-bold" type="b">
      <default>true</default>
    </key>
    <key name="allow-hyperlink" type="b">
      <default>true</default>
    </key>
    <key name="font-name" type="s">
      <default>"Monospace 11"</default>
    </key>
    <key name="limit-scrollback" type="b">
      <default>true</default>
    </key>
    <key name="scrollback-lines" type="u">
      <range min="0" max="999999999"/>
      <default>10000</default>
    </key>
    <key name="scroll-on-output" type="b">
      <default>false</default>
    </key>
    <key name="scroll-on-keystroke" type="b">
      <default>true</default>
    </key>
  </schema>
</schemalist>
